{
  "name": "vana-planner",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc -p tsconfig.build.json",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "cross-env STORYBOOK=1 storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint src --ext .ts,.vue",
    "lint:style": "stylelint \"src/**/*.{scss,vue}\"",
    "lint:all": "npm-run-all2 lint lint:style",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "echo skipped (manual husky)"
  },
  "dependencies": {
    "@vueuse/head": "^2.0.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-links": "^9.0.16",
    "@storybook/addon-onboarding": "^9.0.16",
    "@storybook/addon-vitest": "^9.0.16",
    "@storybook/vue3-vite": "^9.0.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.32",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.53.2",
    "postcss-html": "^1.8.0",
    "sass-embedded": "^1.89.2",
    "storybook": "^9.0.16",
    "stylelint": "^16.21.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-scss": "^6.12.1",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.10"
  },
  "overrides": {
    "@rollup/rollup": "4.16.3"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix"
    ],
    "*.{scss,vue}": [
      "stylelint --fix"
    ],
    "*.{ts,vue,scss,md,json}": [
      "prettier --write"
    ]
  }
}
